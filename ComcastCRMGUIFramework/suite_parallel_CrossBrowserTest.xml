<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite thread-count="2" parallel="tests" name="Parallel-CrossBrowser Suite">
	<test name="TestRunner-FireFox">
		<parameter name="Browser" value="firefox" />
		<classes>
			<class name="com.comcast.crm.campaignTest.CampaignModuleTest" />
			<class name="com.comcast.crm.contactTest.ContactModuleTest" />
			<class name="com.comcast.crm.leadsTest.LeadsModuleTest" />
			<class name="com.comcast.crm.orgTest.OrganizationModuleTest" />
			<class name="com.comcast.crm.priceBooksTest.PriceBooksModuleTest" />
			<class name="com.comcast.crm.productsTest.ProductsModuleTest" />
			<class name="com.comcast.crm.purchaseOrderTest.PurchaseOrderModuleTest" />
			<class name="com.comcast.crm.vendorTest.VendorModuleTest" />
		</classes>
	</test> <!-- Test -->
	<test name="TestRunner-Chrome">
		<parameter name="Browser" value="chrome" />
		<classes>
			<class name="com.comcast.crm.campaignTest.CampaignModuleTest" />
			<class name="com.comcast.crm.contactTest.ContactModuleTest" />
			<class name="com.comcast.crm.leadsTest.LeadsModuleTest" />
			<class name="com.comcast.crm.orgTest.OrganizationModuleTest" />
			<class name="com.comcast.crm.priceBooksTest.PriceBooksModuleTest" />
			<class name="com.comcast.crm.productsTest.ProductsModuleTest" />
			<class name="com.comcast.crm.purchaseOrderTest.PurchaseOrderModuleTest" />
			<class name="com.comcast.crm.vendorTest.VendorModuleTest" />
		</classes>
	</test> <!-- Test -->
</suite> <!-- Suite -->
